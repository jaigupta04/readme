{
  "version": 2,
  "builds": [
    {
      "src": "readme-vue/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "app/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "/app/index.js"
    },
    { "src": "/(.*)", "dest": "/readme-vue/$1" }
  ]
}
